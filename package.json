{
	"name": "waves",
	"displayName": "Waves",
	"publisher": "Mullets",
	"description": "watch as the waves crash, a Roblox Luau theme",
	"repository": "https://github.com/Mullets-Gavin/Waves",
	"icon": "assets/icon.png",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Waves",
				"uiTheme": "vs-dark",
				"path": "./themes/waves.json"
			}
		],
		"grammars": [
			{
				"injectTo": [
					"source.lua"
				],
				"scopeName": "source.injection",
				"path": "./syntaxes/lua.json"
			}
		],
		"semanticTokenScopes": [
			{
				"language": "lua",
				"scopes": {
					"luau.keyword.lua": [
						"luau.keyword.lua"
					]
				}
			}
		]
	}
}
